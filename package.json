{
  "name": "@photobooth-ng/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "build": "nx run-many --target=build --all --configuration=production",
    "build:backend": "nx build backend --configuration=production",
    "build:frontend": "nx build frontend --configuration=production",
    "serve": "nx run-many --target=serve --all --parallel",
    "serve:backend": "nx serve backend",
    "serve:frontend": "nx serve frontend",
    "test": "nx run-many --target=test --all",
    "test:e2e": "npx playwright test",
    "lint": "nx run-many --target=lint --all",
    "deploy:pi": "./scripts/deploy-raspberry-pi.sh",
    "install:pi": "./scripts/install-raspberry-pi.sh",
    "docker:dev": "docker-compose -f docker-compose.dev.yml up",
    "docker:prod": "docker-compose -f docker-compose.prod.yml up -d",
    "docker:build": "docker-compose -f docker-compose.prod.yml build",
    "docker:down": "docker-compose down",
    "migration:generate": "npm run typeorm -- migration:generate",
    "migration:run": "npm run typeorm -- migration:run",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "docs:generate": "compodoc -p ./backend/tsconfig.json -d ./docs/api --theme stripe --hideGenerator --disableGraph",
    "docs:serve": "compodoc -s -d ./docs/api",
    "docs:build": "npm run docs:generate && npm run docs:jekyll",
    "docs:jekyll": "cd ./docs && bundle exec jekyll build"
  },
  "private": true,
